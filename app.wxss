.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.top-title {
  width: 100%;
  height: 60rpx;
  background-color: rgb(218, 218, 218, 0.35);
  font-size: 16px;
  font-weight: bold;
  color: #353535;
  letter-spacing: 8rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.daikaifa {
  width: 100%;
  height: 400px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.daikaifa image {
  width: 200px;
  height: 200px;
}
